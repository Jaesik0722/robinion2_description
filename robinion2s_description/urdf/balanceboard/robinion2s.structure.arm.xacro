				<geometry>
					<mesh filename="package://robinion2s_description/meshes/visual/standard/l_arm_3_visual.stl"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0.000000 0.018081 -0.023869" rpy="0.00 0.00 0.00"/>
				<mass value="0.246669"/>
				<inertia ixx="0.000135" ixy="0.000000" ixz="0.000000"
						 iyy="0.000064" iyz="-0.000001"
						 izz="0.000121" />
			</inertial>
		</link>

		<!-- <joint name="l_wrist_fixed_joint" type="fixed">
			<parent link="l_arm_3_link"/>
			<child link="l_wrist"/>
			<origin xyz="0.000 0.068 -0.0245" rpy="0 0 0"/>
