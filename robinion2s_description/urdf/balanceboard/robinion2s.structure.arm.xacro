					<mesh filename="package://robinion2s_description/meshes/visual/standard/left_gripper_visual.stl"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0.000002 0.043774 0.003217" rpy="0.00 0.00 0.00"/>
				<mass value="0.216195"/>
				<inertia ixx="0.000183" ixy="0.000000" ixz="0.000000"
						 iyy="0.000104" iyz="0.000015"
						 izz="0.000191" />
			</inertial>
		</link>

	</xacro:macro>

