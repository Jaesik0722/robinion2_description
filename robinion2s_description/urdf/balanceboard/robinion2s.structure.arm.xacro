	<!-- <xacro:property name="pi" value="3.141592654" /> -->

	<xacro:macro name="robinion_l_arm" params="parent">
		<joint name="l_shoulder_pitch_joint" type="revolute">
			<parent link="${parent}"/>
			<child link="l_shoulder_link"/>
			<origin xyz="-0.02089 0.0815 0.1388" rpy="0 0.0873 0"/>
			<axis xyz="0 1 0"/>
			<limit velocity="3.14" effort="10.6" lower="${-pi*0.9}" upper="${pi*0.9}" />
			<!-- <limit velocity="3.14" effort="100" lower="-3" upper="3" /> -->
			<dynamics damping="0.1" friction="0.0" />
			<implicitSpringDamper>True</implicitSpringDamper>
		</joint>
		<xacro:insert_transmission name="l_shoulder_pitch_joint" />
	
		<link name="l_shoulder_link">
			<collision>
				<origin xyz="0.00 0.00 0.00" rpy="0.00 0.00 0.00"/>
				<geometry>
					<mesh filename="package://robinion2s_description/meshes/collision/standard/l_shoulder_collision.stl"/>
				</geometry>
			</collision>
			<visual>
				<origin xyz="0.00 0.00 0.00" rpy="0.00 0.00 0.00"/>
				<geometry>
					<mesh filename="package://robinion2s_description/meshes/visual/standard/left_shoulder_visual.stl"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0.000000 0.010235 -0.004313" rpy="0.00 0.00 0.00"/>
				<mass value="0.044923"/>
				<inertia ixx="0.000013" ixy="0.000000" ixz="0.000000"
						 iyy="0.000027" iyz="-0.000003"
						 izz="0.000025" />
			</inertial>
		</link>
	
		<joint name="l_shoulder_roll_joint" type="revolute">
			<parent link="l_shoulder_link"/>
			<child link="l_arm_1_link"/>
			<origin xyz="0.0245 0.030017 -0.022" rpy="-1.3439 0 0"/>
			<axis xyz="-1 0 0"/>
			<limit velocity="3.14" effort="10.6" lower="${-pi*0.5}" upper="${pi*0.5}" />
			<!-- <limit velocity="3.14" effort="100" lower="-3" upper="3" /> -->
			<dynamics damping="0.1" friction="0.0" />
			<implicitSpringDamper>True</implicitSpringDamper>
		</joint>
		<xacro:insert_transmission name="l_shoulder_roll_joint" />
	
		<link name="l_arm_1_link">
			<collision>
				<origin xyz="0.00 0.00 0.00" rpy="0.00 0.00 0.00"/>
				<geometry>
					<mesh filename="package://robinion2s_description/meshes/collision/standard/l_arm_1_collision.stl"/>
				</geometry>
			</collision>
			<visual>
				<origin xyz="0.00 0.00 0.00" rpy="0.00 0.00 0.00"/>
				<geometry>
					<mesh filename="package://robinion2s_description/meshes/visual/standard/l_arm_1_visual.stl"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="-0.027891 0.046707 0.000000" rpy="0.00 0.00 0.00"/>
				<mass value="0.389041"/>
				<inertia ixx="0.000698" ixy="-0.000079" ixz="0.000000"
						 iyy="0.000117" iyz="0.000000"
						 izz="0.000753" />
			</inertial>
		</link>
	
		<joint name="l_elbow_pitch_joint" type="revolute">
			<parent link="l_arm_1_link"/>
			<child link="l_arm_2_link"/>
			<origin xyz="-0.0245 0.118 0.000" rpy="0 0 0"/>
			<axis xyz="0 1 0"/>
